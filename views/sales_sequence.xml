<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <record id="sequence_sales_id" model="ir.sequence">
        <field name="name">sequence_sales_id</field>
        <field name="code">kedai.kopi.sales</field>
        <field name="prefix">%(day)s%(month)s/%(year)s/KOOEFYIDN/</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="True" name="use_date_range"/>
        <field eval="False" name="company_id"/>
        <field name="padding">5</field>
    </record>
</odoo>